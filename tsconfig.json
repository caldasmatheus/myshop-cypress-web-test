{
   "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": [
         "cypress",
         "node"
      ],
      "resolveJsonModule": true,
   },
   "include": [
      "cypress/**/*.ts"
   ]
}